#!/usr/bin/env fontforge

Open($1);
SelectAll();
SelectFewer(0ub0);
SelectFewer(0ub1);
SelectFewer(0ud7);
SelectFewer(0uf7);
SelectFewer(0uf8);
SelectFewer(0u35e);
SelectFewer(0u360);
SelectFewer(0u3d1);
SelectFewer(0u3d5);
SelectFewer(0u3d6);
SelectFewer(0u3dc);
SelectFewer(0u3dd);
SelectFewer(0u3f0);
SelectFewer(0u3f1);
SelectFewer(0u3f5);
SelectFewer(0u2da);
SelectFewer(0u30a);
SelectFewer(0u391,0u3a9);
SelectFewer(0u3b1,0u3c9);
SelectFewer(0u20,0u7e);
DetachAndRemoveGlyphs();
Generate($2);
Quit(0);
